{
  "name": "generated-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "yarn-install": "yarn --modules-folder ~/fsbc/node_modules install",
    "yarn-add": "yarn --modules-folder ~/fsbc/node_modules add",
    "yarn-dev": "yarn --modules-folder ~/fsbc/node_modules add --dev",
    "yarn-remove": "yarn --modules-folder ~/fsbc/node_modules remove",
    "lint": "~/fsbc/node_modules/.bin/eslint --fix core/**/*.js middlewares/**/*.js tests/**/*.js bin/www",
    "production": "NODE_PATH=~/fsbc/node_modules forever start --append -l ~/fsbc/log/forever.log -o ~/fsbc/log/out.log -e ~/fsbc/log/err.log ./bin/www --color",
    "dev": "NODE_PATH=~/fsbc/node_modules ~/fsbc/node_modules/nodemon/bin/nodemon.js --ignore './config' -L ./bin/www",
    "debug-dev": "NODE_PATH=~/fsbc/node_modules node inspect ./bin/www",
    "test": "NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha --trace-warnings ./tests/data.test.js --timeout 60000 --exit",
    "test-dev": "NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha ./tests/data.test.js --grep devtest --timeout 60000 --exit",
    "debug-test": "NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha debug ./tests/data.test.js",
    "debug-test-dev": "NODE_PATH=~/fsbc/node_modules node ~/fsbc/node_modules/mocha/bin/mocha debug ./tests/data.test.js --grep devtest --timeout 60000 --exit",
    "coverage": "NODE_PATH=~/fsbc/node_modules ./node_modules/.bin/nyc --reporter=text --reporter=lcov ./node_modules/mocha/bin/mocha ./tests/data.test.js --timeout 60000 --exit",
    "backup-worker": "NODE_PATH=~/fsbc/node_modules forever start --append -l ~/fsbc/log/backupForever.log -o ~/fsbc/log/backupOut.log -e ~/fsbc/log/backupErr.log core/storage/backupWorker.js --color",
    "debug-backup-worker": "NODE_PATH=~/fsbc/node_modules node inspect core/storage/backupWorker.js",
    "repl": "NODE_PATH=~/fsbc/node_modules node"
  },
  "dependencies": {
    "async": "^3.1.1",
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "http-status-codes": "^1.4.0",
    "is-absolute-url": "^3.0.3",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.9.1",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "object-hash": "^2.0.2",
    "pify": "^5.0.0",
    "random-item": "^3.0.0",
    "superagent": "^5.2.1",
    "uniqid": "^5.2.0",
    "url-parse-lax": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "supertest": "^4.0.2"
  }
}
